{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
    "FileManager": {
      "RootFolderName": "Uploads",
      "MaxFileSizeBytes": 5242880,
      "AllowedImageExtensions": [ ".jpg", ".png", ".gif" ],
      "AllowedDocumentExtensions": [ ".pdf", ".docx" ],
      "AllowedVideoExtensions": [ ".mp4", ".avi" ]
    },
  

  "AllowedHosts": "*"
}
